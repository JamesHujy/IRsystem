        this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n            })\n            .catch(error => {\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n                this.sentenceList.push(\"test\")\n            })\n        }\n\t}\n}\n\n</script>\n\n\n<style scoped>\n#searchEngine {\n    top: 15%;\n    width: 100%;\n    position: fixed;\n    background-color: transparent;\n    z-index: 1;\n}\n\n#searchEngine h1 {\n    font-size: 50px;\n}\n\n#text {\n    width: 40%;\n    height: 41px;\n    padding: 0px;\n    margin:-1px 0 0 0px;\n    border: solid 1px #39F;\n    font-size:24px; \n}\n\n</style>"]}]}